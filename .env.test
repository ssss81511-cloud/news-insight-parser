# ТВОИ ДАННЫЕ ДЛЯ ТЕСТИРОВАНИЯ

# Groq API (ВСТАВЬ СВОЙ КЛЮЧ!)
GROQ_API_KEY=your_groq_api_key_here

# Telegram (ВСТАВЬ СВОИ ДАННЫЕ!)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHANNEL_ID=@your_channel_here

# Автоматизация
AUTO_GENERATE_ENABLED=true
AUTO_GENERATE_HOUR=9
AUTO_GENERATE_MINUTE=0

# Настройки
TOPIC_EXCLUDE_DAYS=30
TOPIC_PREFER_TRENDING=true
CONTENT_LANGUAGE=ru
CONTENT_FORMAT=long_post
ENABLE_REEL=true
